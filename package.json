{
  "name": "shelby",
  "title": "Shelby",
  "version": "0.2.0",
  "description": "Set of highly extensible objects to quickly build Knockout view models that will handle most of your application business cases.",
  "main": "/dist/shelby.js",
  "homepage": "https://github.com/patricklafrance/shelby",
  "author": "Patrick Lafrance",
  "repository": {
    "type": "git",
    "url": "https://github.com/patricklafrance/shelby.git"
  },
  "keywords": [
    "ko",
    "knockout",
    "javascript",
    "view",
    "models"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "bugs": {
    "url": "https://github.com/patricklafrance/shelby/issues"
  },
  "browser": {
    "jquery": "./lib/jquery-1.10.2.js",
    "knockout": "./lib/knockout-3.2.0.js",
    "knockout.viewmodel": "./lib/knockout.viewmodel"
  },
  "browserify-shim": {
