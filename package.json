    "gulp-replace": "^0.3.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-uglify": "^0.3.1",
    "jshint-summary": "^0.4.0",
    "run-sequence": "^0.3.6",
    "vinyl-source-stream": "^0.1.1"
  },
  "scripts": {
